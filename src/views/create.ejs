<!-- Typography -->
<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="stylesheet" href="/css/create.css">

<section id="characterCreator">
  <div class="creator-grid">
    <!-- LEFT COLUMN: THE LEDGER -->
    <div class="ledger-container">
      <!-- TABS -->
      <div id="primaryButtons">
        <ul>
          <li><button type="button" id="gender" class="button main active-tab"><span><i class="fa-solid fa-shirt"></i>
                Style </span></button></li>
          <li><button type="button" id="about" class="button main"><span><i class="fa-solid fa-feather-pointed"></i>
                About </span></button></li>
          <li><button type="button" id="kinks" class="button main"><span><i class="fa-solid fa-heart"></i> Kinks
              </span></button></li>
          <li><button type="button" id="vore" class="button main"><span><i class="fa-solid fa-utensils"></i> Vore
              </span></button></li>
        </ul>
      </div>

      <!-- FORM CONTENT -->
      <% if (charList=='new' ) { %>
        <form action="/api/user/createcharacter" method="POST">
          <% } else {%>
            <form action="/api/user/editcharacter" method="POST">
              <%}%>

                <div id="genderPullout" class="pullout">
                  <!-- Body Shape -->
                  <button type="button" class="collapsible"><span><i class="fa-solid fa-venus-mars"></i> Base
                      Shape</span> <i class="fa-solid fa-chevron-down"></i></button>
                  <div class="style-grid active">
                    <div class="selectionContainer">
                      <div class="input-group">
                        <label>Shape:</label>
                        <select name="bodyShape" id="bodyShape">
                          <option value="F-">Feminine</option>
                          <option value="M-">Masculine (unavailable)</option>
                        </select>
                      </div>
                    </div>
                    <div class="selectionContainer">
                      <div class="input-group">
                        <label>Anatomy:</label>
                        <select name="genitles" id="genitles">
                          <option value="empty">Vagina (unavailable)</option>
                          <option value="empty">Penis (unavailable)</option>
                          <option value="empty">Both (unavailable)</option>
                        </select>
                      </div>
                    </div>
                  </div>



